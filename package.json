{
  "name": "angularjs-es6",
  "version": "0.1.0",
  "description": "An ES6 implementation of angular.js, with babel and webpack.",
  "keywords": [
    "angularjs",
    "es6",
    "webpack",
    "babel"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint src test",
    "test": "karma start test/karma.conf.js --single-run",
    "test:watch": "karma start test/karma.conf.js --auto-watch",
    "build": "browserify src/bootstrap.js > myangular.js",
    "build:minified": "browserify src/bootstrap.js | uglifyjs -mc > myangular.min.js"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "karma": "^0.13.22",
    "karma-eslint": "^2.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "simple-assign": "^0.1.0",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AJIh/angular-es6.git"
  },
  "dependencies": {
    "jquery": "^3.0.0",
    "lodash": "^4.13.1"
  }
}
